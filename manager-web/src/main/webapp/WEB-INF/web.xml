<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>com.tzjf.web.MyListener</listener-class>
  </listener>

  <servlet>
    <servlet-name>addUser</servlet-name>
    <servlet-class>com.tzjf.web.AddUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addUser</servlet-name>
    <url-pattern>/adduser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CloseSessionServlet</servlet-name>
    <servlet-class>com.tzjf.web.CloseSessionServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>CloseSessionServlet</servlet-name>
    <url-pattern>/closeSession</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JsonDataServlet</servlet-name>
    <servlet-class>com.tzjf.web.JsonDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JsonDataServlet</servlet-name>
    <url-pattern>/jsonData</url-pattern>
  </servlet-mapping>
</web-app>
